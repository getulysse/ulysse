{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "max-len": [0],
        "object-curly-newline": [0],
        "import/no-extraneous-dependencies": [0],
        "import/prefer-default-export": [0],
        "no-restricted-syntax": [0],
        "complexity": [2, 6],
        "max-depth": [2, 2],
        "import/extensions": [0],
        "no-console": [0]
    }
}
